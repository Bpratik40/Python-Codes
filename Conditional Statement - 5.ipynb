    "#Grading System\n",
    "grade = int(input(\"Enter A Marks: \"))\n",
    "if(grade<25):\n",
    "    print(\"F\")\n",
    "elif(grade>=25 and grade<45):\n",
    "    print(\"E\")\n",
    "elif(grade>=45 and grade<50):\n",
    "    print(\"D\")\n",
    "elif(grade>=50 and grade<60):\n",
    "    print(\"C\")\n",
    "elif(grade>=60 and grade<80):\n",
    "    print(\"B\")\n",
    "else:\n",
    "    print(\"A\")\n",
    " "
